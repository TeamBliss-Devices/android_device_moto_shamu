
restorecon_recursive /sys/devices/system/cpu/cpufreq
on charger
 #disable zen decision
    write /sys/kernel/zen_decision/enabled 0

 #low power governing
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor dynamic_interactive
    write /sys/devices/system/cpu/cpu1/online 1
    write /sys/devices/system/cpu/cpu2/online 1
    write /sys/devices/system/cpu/cpu3/online 1
    write /sys/module/lpm_levels/enable_low_power/l2 4
    write /sys/module/msm_pm/modes/cpu0/power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu0/power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu0/standalone_power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu0/standalone_power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu0/retention/idle_enabled 1

on boot
    # Shamu boots with performance governor.
    # Switch one core to performance to set permissions, for power hal and system server.
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor performance
    chown system system /sys/devices/system/cpu/cpufreq/performance/timer_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/timer_rate
    chown system system /sys/devices/system/cpu/cpufreq/performance/timer_slack
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/timer_slack
    chown system system /sys/devices/system/cpu/cpufreq/performance/min_sample_time
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/min_sample_time
    chown system system /sys/devices/system/cpu/cpufreq/performance/hispeed_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/hispeed_freq
    chown system system /sys/devices/system/cpu/cpufreq/performance/target_loads
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/target_loads
    chown system system /sys/devices/system/cpu/cpufreq/performance/go_hispeed_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/go_hispeed_load
    chown system system /sys/devices/system/cpu/cpufreq/performance/above_hispeed_delay
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/above_hispeed_delay
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/boostpulse_duration
    chown system system /sys/devices/system/cpu/cpufreq/performance/io_is_busy
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/io_is_busy
    chown system system /sys/devices/system/cpu/cpufreq/performance/sampling_down_factor
    chmod 0660 /sys/devices/system/cpu/cpufreq/performance/sampling_down_factor

on property:dev.bootcomplete=1
    write /sys/module/lpm_levels/enable_low_power/l2 4
    write /sys/module/msm_pm/modes/cpu0/power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu1/power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu2/power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu3/power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu0/power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu1/power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu2/power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu3/power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu0/standalone_power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu1/standalone_power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu2/standalone_power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu3/standalone_power_collapse/suspend_enabled 1
    write /sys/module/msm_pm/modes/cpu0/standalone_power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu1/standalone_power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu2/standalone_power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu3/standalone_power_collapse/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu0/retention/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu1/retention/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu2/retention/idle_enabled 1
    write /sys/module/msm_pm/modes/cpu3/retention/idle_enabled 1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor dynamic_interactive
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor dynamic_interactive
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor dynamic_interactive
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor dynamic_interactive
    write /sys/module/adreno_idler/parameters/adreno_idler_active Y
    write /sys/module/msm_thermal/parameters/enabled Y
    write /sys/module/msm_thermal/parameters/limit_temp_degC 70
    write /sys/module/msm_thermal/core_control/enabled 1

    # cpufreq settings
    write /sys/devices/system/cpu/cpufreq/performance/up_threshold 95
    write /sys/devices/system/cpu/cpufreq/performance/down_threshold 30
    write /sys/devices/system/cpu/cpufreq/performance/freq_step 5
    write /sys/devices/system/cpu/cpufreq/performance/sampling_down_factor 1
    write /sys/devices/system/cpu/cpufreq/performance/sampling_rate 2000
    write /sys/devices/system/cpu/cpufreq/performance/input_boost_freq 1497600

    # Mako
    write /sys/class/misc/mako_hotplug_control/load_threshold 0
    write /sys/class/misc/mako_hotplug_control/high_load_counter 10
    write /sys/class/misc/mako_hotplug_control/max_load_counter 20
    write /sys/class/misc/mako_hotplug_control/cpufreq_unplug_limit 1800000
    write /sys/class/misc/mako_hotplug_control/min_time_cpu_online 1
    write /sys/class/misc/mako_hotplug_control/timer 1

    # MSM Hotplug
    write /sys/module/msm_hotplug/min_cpus_online 1
    write /sys/module/msm_hotplug/down_lock_duration 500
    write /sys/module/msm_hotplug/boost_lock_duration 2500
    write /sys/module/msm_hotplug/update_rates "200 5:100 50:50 350:200"
    write /sys/module/msm_hotplug/fast_lane_load 100

    # Bricked
    write /sys/kernel/msm_mpdecision/conf/enabled 0
    write /sys/kernel/msm_mpdecision/conf/min_cpus_online 1
    write /sys/kernel/msm_mpdecision/conf/min_cpus_online_susp 1
    write /sys/kernel/msm_mpdecision/conf/max_cpus_online 4
    write /sys/kernel/msm_mpdecision/conf/max_cpus_online_susp 1
    write /sys/kernel/msm_mpdecision/conf/suspend_defer_time 10
    write /sys/kernel/msm_mpdecision/conf/idle_freq 499200
    write /sys/kernel/msm_mpdecision/conf/startdelay 20000
    write /sys/kernel/msm_mpdecision/conf/delay 20000
